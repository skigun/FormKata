<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="acme.form.type.article" class="Acme\KataBundle\Form\Type\ArticleType">
            <tag name="form.type" alias="article" />
        </service>
        <service id="acme.form.type.user" class="Acme\KataBundle\Form\Type\UserType">
            <tag name="form.type" alias="user" />
        </service>
        <service id="acme.form.type.location" class="Acme\KataBundle\Form\Type\LocationType">
            <tag name="form.type" alias="location" />
        </service>
    </services>
</container>
